/* ---Global Styles--- */
* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

body {
    font-family: "Roboto", sans-serif;
    line-height: 1.65;
}

:root {
    --primaryColor: #00a3e1;
    --secondaryColor: #17181b;
    --backgroundColor: #2c2d2f;
    --borderText: #7a7a7a;
    --hoverColor: #fff;
    --blackColor: black;
}

/* ---Side Nav--- */

a {
    text-decoration: none;
}

li {
    list-style: none;
}

.sidebar-main {
    display: flex;
    flex-direction: column;
    height: 100vh;
    width: 200px;
    top: 0;
    left: 0;
    position: fixed;
    z-index: 10;
}

/* imag area css */


.image-area {
    width: 200px;
    height: 200px;
    position: relative;
    display: flex;
    flex-direction: column;
}

.image-area img {
    width: 100%;
    height: 270px;
    object-fit: contain;
    z-index: -1;
}

.person-name {
    width: 200px;
    height: 45px;
    background-color: var(--primaryColor);
    position: absolute;
    bottom: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    opacity: 0.9;


}

.person-name span {
    font-size: 22px;
    color: var(--hoverColor);
    font-family: 'Playfair Display', serif;
    font-weight: 600;
    text-shadow: 1px 9px 12px #000;
}

/* menu bar css starts here  */

.menubar-area {
    display: flex;
    flex-direction: column;
    /* padding-left: 10px; */
}

.menubar-area ul {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

/* footer css starts here */

.footer-area {
    display: flex;
    flex-direction: column;
    justify-content: flex-end;
    align-items: center;
    background-color: var(--secondaryColor);
    height: 100vh;
}

.footer-icons {
    font-size: 18px;
    margin-top: 17px;
    color: rgb(156, 156, 156);
}

.footer-area .footer-text {
    font-size: 12px;
    margin-top: 7px;
    color: rgb(146, 144, 144);
    text-align: center;
    line-height: 15px;
    margin-bottom: 1rem;
}

.icons-social {
    padding: 0px 10px 10px 0px;
    transition: 0.3s;
}

.icons-social:hover {
    transition: 0.3s;
    color: var(--hoverColor);
    cursor: pointer;
}

/* ---Section About--- */

.home-container {
    display: flex;
    justify-content: center;
    align-items: center;
}

.main-image-area {
    width: 100%;
    height: 100vh;
    background: rgb(0, 0, 0, 0.5) url('/public/home-bg.jpg');
    background-blend-mode: darken;
    background-position: center;
    background-size: cover;
    background-repeat: no-repeat;
    background-position-x: center;
    background-position-y: center;
    display: flex;
    flex-direction: column;
    align-items: center;
    object-fit: contain;
    margin-left: 200px;
}

/* ---Section About--- */

.about-container {
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: var(--backgroundColor);
    margin-left: 200px;
}

.main-about-area {
    width: 100%;
    height: auto;
    display: flex;
    flex-direction: column;
    align-items: center;
    /* object-fit: contain; */
    margin-left: 65px;
    margin-right: 65px;
    border: 1px solid red;
    font-family: 'Playfair Display', serif;
}

.banner-text {
    position: absolute;
    color: #00a3e1;
    font-size: 4.1rem;
    margin-left: 150px;
    margin-bottom: 100px;
    letter-spacing: 2px;
    font-weight: 700px;
    font-family: 'Playfair Display', serif;
    font-weight: bold;


}

.banner-qandeel {
    color: white;
    margin: 0px 10px;
}

.banner-khan {
    color: #00a3e1;
    margin: 0px 10px;
}

/* ---------ABOUT PAGE STARTS HERE----------- */


.about-head-logo-area {
    width: 100%;
    height: auto;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1px var(--borderText) dashed;
    margin-top: 35px;
}

.aboutme-heading {
    font-size: 36px;
    letter-spacing: 1.5px;
    color: var(--primaryColor);
}

.aboutuser-icon {
    height: 50px;
    width: 50px;
    border: 1px solid red;

}

.about-user-icon {
    height: 40px;
    width: 40px;
    fill: #00a3e1;
    /* background-color: #00a3e1; */
}

.about-all-info {
    display: flex;
    justify-content: start;
    /* align-items: center; */
    /* flex-wrap: wrap; */
    width: 100%;
    height: 100vh;
    /* border: 1px solid red; */
    /* margin-top: 25px; */
}

.about-left-info {
    width: 60%;
    height: auto;

}

.ui-ux-designer {
    color: white;
    margin-bottom: 25px;
    font-size: 22px;
    margin-top: 35px;
}

.about-right-info {
    width: 40%;
    height: auto;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: start;
    margin-top: 35px;


}

.personel-info-heading {
    color: white;
    font-family: "Playfair Display", serif;
    font-weight: 700;
    font-size: 22px;
    margin-bottom: 20px;
    margin-right: 47px;
}

.personal-info-list {
    color: white;
    line-height: 1em;
    margin-bottom: 15px;
}

.personal-info-title {
    font-family: "Playfair Display", serif;
    font-size: 16px;
    font-weight: 700;
    /* letter-spacing: .5px; */
    position: relative;
    display: inline-block;
    margin-right: 15px;
    padding-bottom: 1px;
    border-bottom: 3px solid #00a3e1;
    color: #fff;
}

.personal-info-value {
    font-size: 16px;
    /* font-weight: 700; */
    letter-spacing: .5px;
    position: relative;
    display: inline-block;
    margin-right: 15px;
    padding-bottom: 1px;
    color: #fff;
    font-family: Arial, Helvetica, sans-serif;
}

.uiux-dummy-text {
    color: white;
    font-family: Arial, Helvetica, sans-serif;

}

.personal-info-title ::after {
    content: ' :';
    top: 0;
    position: absolute;
    right: -10px;
}

.about-right-info ul li {
    padding: 3px 0px;
}

.download-resume-btn {
    color: var(--primaryColor);
    border: 2px solid #0082b6;
    font-family: Arial, Helvetica, sans-serif;
    margin: 20px 0px;
    margin-right: 70px;
    padding: 6px 18px;
    font-size: 14px;
    position: relative;
    transition: ease-out 0.5s;
    z-index: 1;
}

.download-resume-btn:hover {
    color: white;
}

.download-resume-btn::after {
    content: "";
    left: 90%;
    right: 90%;
    top: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
    background-color: #0082b6;
    position: absolute;
    transition: 0.5s all ease;
}

.download-resume-btn:hover::after {
    left: 0;
    right: 0;
    opacity: 1;
    transition: 0.5s all ease;
    z-index: -1;
}

.signature-area img {
    width: 100%;
    height: 100px;
    transform: translateX(-180px);
    filter: brightness(0) invert(1);
}

.signature-area {
    margin-top: 10px;
    z-index: 1;
}

/* ---------SERVICES AREA CSS START---------- */



.service-area {
    display: flex;
    flex-direction: column;
    /* background-color: var(--backgroundColor); */
    /* border: 2px solid red; */
    
}

.services {
    display: flex;
    justify-content: stretch;
    align-items: center;
    
}

.skills {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    /* padding: 15px 0px; */
    /* margin: 15px 15px; */
}
.skills-heading{
    color: white;
    font-size: 17px;
    font-weight: bold;
    padding: 10px 0px;

}
.skills-description{
    color: #d4d4d4;
    text-align: center;
    padding: 10px 0px;


}

